#include<stdio.h>
#include<stdlib.h>
#include<string.h>


void exibirArray(int Array [], int tamanho);

void main(){

    system("cls");
    int v[] = {7, 3, 1, 9, 6};
    int tamanho = sizeof(v) / sizeof(int);
    exibirArray(v, tamanho);
    printf("main: %d\n", tamanho);

}


void exibirArray(int Array [], int tamanho){
    for (int i = 0; i < tamanho; i++) printf("%d\t", Array[i]);
    printf("\n");
    tamanho++;
    
}
